{
  "name": "yc-directory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "predev": "npm run typegen",
    "prebuild": "node -e \"if (process.env.VERCEL !== '1') require('child_process').execSync('npm run typegen', {stdio: 'inherit'})\"",
    "typegen": "sanity schema extract --path=./src/sanity/schema.json && sanity typegen generate --schema=./src/sanity/schema.json",
    "check-lint": "eslint .",
    "lint-staged": "lint-staged",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "vercel-build": "npm install --legacy-peer-deps && next build"
  },
  "overrides": {
    "@sanity/ui": {
      "exact": "2.16.7"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.6",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^1.1.0",
    "@sanity/ui": "^2.16.7",
    "@sanity/vision": "^3.95.0",
    "@sentry/nextjs": "^9.33.0",
    "@tailwindcss/typography": "^0.5.16",
    "@uiw/react-md-editor": "^4.0.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "easymde": "^2.20.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.523.0",
    "markdown-it": "^14.1.0",
    "next": "15.3.4",
    "next-auth": "^5.0.0-beta.29",
    "next-sanity": "^9.12.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-is": "^19.1.1",
    "sanity": "^3.95.0",
    "sanity-plugin-markdown": "^5.1.1",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "sonner": "^2.0.5",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "prettier": "^2.8.8",
    "sanity-codegen": "^0.9.8",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
